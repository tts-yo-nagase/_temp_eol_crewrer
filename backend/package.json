{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npx prisma generate && nest build",
    "vercel-build": "npx prisma generate && nest build",
    "postinstall": "npx prisma generate",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch --turbopack",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:prod:docker": "pnpm run setup && pnpm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "setup": "npx prisma migrate deploy && npx prisma generate && npx prisma db seed",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run test:db:up && DATABASE_URL=postgresql://postgres:postgres@localhost:5433/app_test_db JWT_SECRET=test-jwt-secret-key-for-testing-only NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:watch": "pnpm run test:db:up && DATABASE_URL=postgresql://postgres:postgres@localhost:5433/app_test_db JWT_SECRET=test-jwt-secret-key-for-testing-only NODE_ENV=test jest --config ./test/jest-e2e.json --watch --runInBand",
    "test:db:up": "docker compose up -d postgres-test && sleep 3",
    "test:db:down": "docker compose down postgres-test",
    "test:db:setup": "DATABASE_URL=postgresql://postgres:postgres@localhost:5433/app_test_db npx prisma migrate deploy"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "consola": "^3.4.2",
    "express": "^5.2.0",
    "next-auth": "^4.24.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
